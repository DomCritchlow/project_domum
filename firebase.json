{
  "hosting": {
    "public": "public",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [
      {
        "source": "**",
        "destination": "/index.html"
      }, {
        "source":"/about",
        "destination":"/about.html"
      }
    ],
    "cleanUrls": true
  }
}
